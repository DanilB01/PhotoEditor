<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobphotoedit">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CubeActivity"/>
        <activity android:name=".FilteringActivity" />
        <activity android:name=".UnsharpMaskingActivity" />
        <activity android:name=".RetouchingActivity" />
        <activity android:name=".LineDrawingActivity" />
        <activity android:name=".SegmentationActivity" />
        <activity android:name=".ImageScalingActivity" />
        <activity android:name=".CorrectionActivity" />
        <activity android:name=".ImageRotationActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".DesktopActivity" />
    </application>

</manifest>